{
  "beg": {
    "prefix": "beg",
    "description": "LaTex begin",
    "body": [
      "\\begin{$1\\}",
      "\t$0",
      "\\end{$1\\}"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "math": {
    "prefix": "math",
    "description": "Markdown LaTex",
    "body": [
      "\\$\\$",
      "$1",
      "\\$\\$"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "inl": {
    "prefix": "inl",
    "description": "Markdown inline LaTex",
    "body": "\\$$1\\$",
    "luasnip": {
      "autotrigger": true
    }
  },
  "al": {
    "prefix": "al",
    "description": "LaTex align",
    "body": [
      "\\begin{align\\}",
      "$1",
      "\\end{align\\}"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "al*": {
    "prefix": "al*",
    "description": "LaTex align*",
    "body": [
      "\\begin{align*\\}",
      "$1",
      "\\end{align*\\}"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "lim": {
    "prefix": "lim",
    "description": "LaTex limit",
    "body": "\\lim_{$1 \\to $2\\} f($3) = $4",
    "luasnip": {
      "autotrigger": true
    }
  },
  "wise": {
    "prefix": "wise",
    "description": "LaTex Piecewise Function",
    "body": [
      "\\begin{cases\\}",
      "$1, & \\text{if \\} $2\\\\",
      "$3 & \\text{if \\} $4",
      "\\end{cases\\}"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "sr": {
    "prefix": "sr",
    "description": "^2",
    "body": "^2",
    "luasnip": {
      "autotrigger": true
    }
  },
  "cb": {
    "prefix": "cb",
    "description": "^3",
    "body": "^3",
    "luasnip": {
      "autotrigger": true
    }
  },
  "compl": {
    "prefix": "compl",
    "description": "complement",
    "body": "^{c\\}",
    "luasnip": {
      "autotrigger": true
    }
  },
  "td": {
    "prefix": "td",
    "description": "superscript",
    "body": "^{$1\\}$0",
    "luasnip": {
      "autotrigger": true
    }
  },
  "//": {
    "prefix": "//",
    "description": "Fraction",
    "body": "\\frac{$1\\}{$2\\}$0",
    "luasnip": {
      "autotrigger": true
    }
  },
  "/": {
    "prefix": "/",
    "description": "Fraction",
    "body": "\\frac{$TM_SELECTED_TEXT\\}{$1\\}$0",
    "luasnip": {
      "autotrigger": true
    }
  },
  "sympy": {
    "prefix": "sympy",
    "description": "sympy block ",
    "body": "sympy $1 sympy$0"
  },
  "ooo": {
    "prefix": "ooo",
    "description": "Infinity",
    "body": "\\infin",
    "luasnip": {
      "autotrigger": true
    }
  },
  "==": {
    "prefix": "==",
    "description": "equals",
    "body": "&= $1 \\\\",
    "luasnip": {
      "autotrigger": true
    }
  },
  "!=": {
    "prefix": "!=",
    "description": "equals",
    "body": "\\neq ",
    "luasnip": {
      "autotrigger": true
    }
  },
  "->": {
    "prefix": "->",
    "description": "to",
    "body": "\\to ",
    "luasnip": {
      "autotrigger": true,
      "priority": 100
    }
  },
  ">=": {
    "prefix": ">=",
    "description": "greater eq",
    "body": "\\geq",
    "luasnip": {
      "autotrigger": true
    }
  },
  "<=": {
    "prefix": "<=",
    "description": "less eq",
    "body": "\\leq",
    "luasnip": {
      "autotrigger": true
    }
  },
  "root": {
    "prefix": "root",
    "description": "square root",
    "body": "\\sqrt{$1\\}",
    "luasnip": {
      "autotrigger": true
    }
  },
  "~": {
    "prefix": "~",
    "description": "prime",
    "body": "\\prime",
    "luasnip": {
      "autotrigger": true
    }
  }
}
